<MyGridLayout>

	nome:nome
	tipo:tipo
	GridLayout:
		cols:1
		size: root.width, root.height
		GridLayout:
			cols:2
			Label:
				text: "Nome do exame"
			TextInput:
				id: nome
				multiline:False
			Label:
				text: "Tipo do exame"
			TextInput:
				id: tipo
				multiline:False

		Button:
			text: "CADASTRAR EXAME"
			font_size: 32
			on_press: root.press()
